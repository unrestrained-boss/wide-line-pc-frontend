  "scripts": {
    "start": "env-cmd -f .env.dev react-app-rewired start",
    "build:staging": "CI=false && env-cmd -f .env.staging react-app-rewired build",
    "build:prod": "env-cmd -f .env.prod react-app-rewired build",
    "test": "react-app-rewired test",
    "test:coverage": "react-app-rewired test --coverage --watchAll=false",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls",
    "eject": "react-app-rewired eject",
    "precommit-msg": "echo 'Pre-commit checks.........' && exit 0"
  },
  "pre-commit": [
    "precommit-msg",
    "test:coverage"
  ],
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@types/enzyme": "^3.10.3",
    "@types/lodash": "^4.14.137",
    "@types/react-transition-group": "^4.2.2",
    "@types/yup": "^0.26.23",
